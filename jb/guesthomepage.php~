<?php
session_start();
function display($body)
{
sprintf("<body>%s</body>", include $body);
}

function guesthomelogic($session)
{
	//display guest homepage not first time
	return 0;
}


$mode = guesthomelogic($session);
switch($mode)
{
case 0:
//display guest homepage
$body = "homepage.php";
break;
default:
$body = "body1.php";
break;
}

display($body);

?>
